{
  "name": "typescript",
  "version": "0.1.0",
  "description": "This is a simple SOLID project.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest -c jest.config.json",
    "precommit": "npm run lint && npm run test",
    "watch": "npx tsc --watch",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "npx eslint -c .eslintrc.json src/app",
    "lint:fix:src": "npx eslint --fix -c .eslintrc.json src/app",
    "lint:test": "npx eslint -c .eslintrc.json src/test",
    "lint:fix:test": "npx eslint --fix -c .eslintrc.json src/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yusufshakeel/solid-project.git"
  },
  "keywords": [
    "solid",
    "typescript"
  ],
  "author": "Yusuf Shakeel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yusufshakeel/solid-project/issues"
  },
  "homepage": "https://github.com/yusufshakeel/solid-project#readme",
  "engines": {
    "node": ">12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.6",
    "ts-jest": "^29.0.5",
    "typescript": "^4.3.0"
  }
}
